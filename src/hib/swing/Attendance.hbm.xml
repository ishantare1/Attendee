<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="hib.swing"> 
  <class name="Attendance" table="AttendanceTable1">
  <id name="students" column="students"/>
  <property name="reason" column="reason"/>
  <property name="courseName" column="coursename"/>
  <property name="absent" column="absent"/>
  <property name="present" column="present"/>
  <property name="totalCount" column="totalcount"/>
  <property name="date" column="date"/>
  <property name="batchTiming" column="batchtiming"/>
  
 </class>
 </hibernate-mapping>
